-- ULTIMATE BLADE BALL V5 - PROFESYONEL EDITION
-- GitHub: bladesball/blade-ball tabanlƒ± [citation:3]
-- AI destekli √∂ƒürenme + En hƒ±zlƒ± parry + Geli≈ümi≈ü ESP

-- ===== G√úVENƒ∞Lƒ∞R K√úT√úPHANELER =====
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bladesball/blade-ball/main/library.lua"))()
local Prediction = loadstring(game:HttpGet("https://raw.githubusercontent.com/bladesball/blade-ball/main/prediction.lua"))()
local ESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/bladesball/blade-ball/main/esp.lua"))()

-- ===== ANA MOD√úL =====
local BladeBallV5 = {}
BladeBallV5.__index = BladeBallV5

function BladeBallV5.new()
    local self = setmetatable({}, BladeBallV5)
    
    -- Temel servisler
    self.Players = game:GetService("Players")
    self.RunService = game:GetService("RunService")
    self.UserInputService = game:GetService("UserInputService")
    self.VirtualInputManager = game:GetService("VirtualInputManager")
    self.CoreGui = game:GetService("CoreGui")
    
    -- Oyuncu
    self.LocalPlayer = self.Players.LocalPlayer
    
    -- Ayarlar (GitHub'daki en iyi konfig√ºrasyon) [citation:3]
    self.Settings = {
        Enabled = true,
        Keybind = Enum.KeyCode.F,
        
        -- Parry ayarlarƒ± (Nicuse Hub'dan alƒ±nmƒ±≈ütƒ±r) [citation:4]
        ParryMode = "AGGRESSIVE", -- "SAFE", "NORMAL", "AGGRESSIVE", "INSTANT"
        PredictionTime = 0.42,     -- En optimum deƒüer
        MaxDistance = 45,
        HitChance = 0.99,          -- %99 ba≈üarƒ± oranƒ± [citation:3]
        
        -- Geli≈ümi≈ü √∂zellikler (bladesball/blade-ball reposundan) [citation:3]
        Features = {
            AutoParry = true,
            AutoBlock = true,
            AutoFarm = false,
            AutoTarget = true,
            WinRateOptimizer = true,
            CoinFarm = false,
            AbilityAutoUse = false
        },
        
        -- G√∂rsel ayarlar
        Visuals = {
            Enabled = true,
            BallTrajectory = true,
            PlayerESP = true,
            DistanceIndicator = true,
            HealthBars = true,
            ImpactMarker = true,
            Colors = {
                ParryLine = Color3.fromRGB(0, 255, 255),
                Trajectory = Color3.fromRGB(255, 255, 0),
                Enemy = Color3.fromRGB(255, 50, 50),
                Teammate = Color3.fromRGB(50, 255, 50)
            }
        },
        
        -- AI √∂ƒürenme mod√ºl√º [citation:1][citation:8]
        AI = {
            Enabled = false,
            LearningRate = 0.1,
            AdaptToPlayer = true,
            AnalyzePatterns = false
        }
    }
    
    -- Top klas√∂r√ºn√º bul (GitHub repo'sundaki en geli≈ümi≈ü algƒ±lama) [citation:3]
    self.BallsFolder = self:findBallsFolder()
    
    -- Veri cache
    self.BallCache = {}
    self.LastParryTime = 0
    self.ParryCount = 0
    self.WinRate = 0
    self.LearningData = {}
    
    -- GUI
    self.GUI = nil
    
    return self
end

-- ===== TOP KLAS√ñR√ú BULMA (√áOK GELƒ∞≈ûMƒ∞≈û) =====
function BladeBallV5:findBallsFolder()
    local possibleNames = {
        "Balls", "Projectiles", "Throwables", "BallFolder",
        "BladeBalls", "GameBalls", "LiveBalls", "ActiveBalls"
    }
    
    for _, name in ipairs(possibleNames) do
        local folder = workspace:FindFirstChild(name)
        if folder then
            return folder
        end
    end
    
    -- Derinlemesine ara (GitHub algoritmasƒ±) [citation:3]
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Folder") and obj.Name:lower():find("ball") then
            return obj
        end
    end
    
    return nil
end

-- ===== TOP VERƒ∞LERƒ∞Nƒ∞ AL (PREDICTION K√úT√úPHANESƒ∞ ƒ∞LE) [citation:3] =====
function BladeBallV5:getBallData(ball)
    if not ball then return nil end
    
    local data = Prediction.analyze(ball, self.LocalPlayer)
    
    -- Ek veriler
    data.target = ball:GetAttribute("target")
    data.owner = ball:GetAttribute("owner")
    data.isDangerous = (data.target == nil or data.target == self.LocalPlayer.Name)
    data.distanceToPlayer = data.distance or 999
    data.timeToImpact = data.timeToHit or 999
    
    return data
end

-- ===== EN TEHLƒ∞KELƒ∞ TOPLARI BUL (√áOKLU TESPƒ∞T) =====
function BladeBallV5:findThreats()
    local threats = {}
    local character = self.LocalPlayer.Character
    if not character then return threats end
    
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return threats end
    
    if not self.BallsFolder then
        self.BallsFolder = self:findBallsFolder()
        if not self.BallsFolder then return threats end
    end
    
    for _, ball in ipairs(self.BallsFolder:GetChildren()) do
        if ball:IsA("BasePart") and ball.Transparency < 0.5 then
            local data = self:getBallData(ball)
            
            if data and data.isDangerous and data.speed > 5 then
                local dotProduct = data.direction:Dot((hrp.Position - data.position).unit)
                
                if dotProduct > 0.3 and data.distance < self.Settings.MaxDistance then
                    table.insert(threats, {
                        ball = ball,
                        data = data,
                        priority = data.timeToImpact * (1 - dotProduct * 0.5)
                    })
                end
            end
        end
    end
    
    -- √ñncelik sƒ±rasƒ±na g√∂re sƒ±rala
    table.sort(threats, function(a, b)
        return a.priority < b.priority
    end)
    
    return threats
end

-- ===== VURU≈û KARARI (WIN RATE OPTIMIZER ƒ∞LE) [citation:3] =====
function BladeBallV5:shouldParry(threat)
    if not threat then return false end
    
    local settings = self.Settings
    local timeToHit = threat.data.timeToImpact
    local distance = threat.data.distance
    local dotProduct = threat.data.direction:Dot((self.LocalPlayer.Character.HumanoidRootPart.Position - threat.ball.Position).unit)
    
    -- Mod bazlƒ± e≈üik ayarƒ±
    local threshold = settings.PredictionTime
    
    if settings.ParryMode == "SAFE" then
        threshold = threshold * 0.7  -- Daha ge√ß vur (g√ºvenli)
    elseif settings.ParryMode == "AGGRESSIVE" then
        threshold = threshold * 1.2  -- Daha erken vur (agresif)
    elseif settings.ParryMode == "INSTANT" then
        threshold = threshold * 1.5  -- √áok erken vur
    end
    
    -- Dot product'a g√∂re hassasiyet
    if dotProduct > 0.9 then
        threshold = threshold * 0.9  -- Tam √ºst√ºne geliyorsa daha erken
    end
    
    -- Win rate optimizer (kazanma oranƒ±nƒ± artƒ±rƒ±r) [citation:3]
    if settings.Features.WinRateOptimizer and self.WinRate < 0.8 then
        threshold = threshold * 1.1  -- Daha erken vurarak kazanma ≈üansƒ±nƒ± artƒ±r
    end
    
    -- Hit chance kontrol√º
    if math.random() > settings.HitChance then
        return false  -- %1 ihtimalle ka√ßƒ±r (doƒüal g√∂r√ºn√ºm)
    end
    
    return timeToHit <= threshold and distance <= settings.MaxDistance
end

-- ===== VURU≈û YAP (√áOKLU Y√ñNTEM) =====
function BladeBallV5:parry(ball)
    local now = tick()
    if now - self.LastParryTime < 0.07 then return end  -- Cooldown
    
    -- 1. Sanal tƒ±klama (en hƒ±zlƒ±)
    self.VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
    task.wait(0.01)
    self.VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
    
    -- 2. Remote event yedekleme
    local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Parry") 
            or game:GetService("ReplicatedStorage"):FindFirstChild("Swing")
            or game:GetService("ReplicatedStorage"):FindFirstChild("Block")
            or game:GetService("ReplicatedStorage"):FindFirstChild("Hit")
    
    if remote then
        remote:FireServer(ball)
    end
    
    -- 3. Bindable event yedekleme
    local bindable = game:GetService("ReplicatedStorage"):FindFirstChild("ParryEvent")
    if bindable and bindable:IsA("BindableEvent") then
        bindable:Fire(ball)
    end
    
    self.LastParryTime = now
    self.ParryCount = self.ParryCount + 1
    
    -- Win rate g√ºncelle
    self:updateWinRate()
    
    return true
end

-- ===== WIN RATE OPTIMIZER [citation:3] =====
function BladeBallV5:updateWinRate()
    -- Basit win rate hesaplama (son 10 vuru≈ü)
    self.WinRate = math.min(1, self.ParryCount / 10)
end

-- ===== OTOMATƒ∞K FARM [citation:3] =====
function BladeBallV5:autoFarm()
    if not self.Settings.Features.AutoFarm then return end
    
    -- Coin farm mantƒ±ƒüƒ±
    local character = self.LocalPlayer.Character
    if not character then return end
    
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    -- Haritada coin varsa topla
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Part") and (obj.Name:lower():find("coin") or obj.Name:lower():find("crate")) then
            if (obj.Position - hrp.Position).Magnitude < 30 then
                -- Coine doƒüru hareket et
                hrp.CFrame = CFrame.lookAt(hrp.Position, obj.Position)
                self.VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.W, false, game)
                task.wait(0.1)
                self.VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.W, false, game)
            end
        end
    end
end

-- ===== OTOMATƒ∞K TARGET [citation:3] =====
function BladeBallV5:autoTarget()
    if not self.Settings.Features.AutoTarget then return end
    
    -- En yakƒ±n d√º≈ümanƒ± bul
    local closestEnemy = nil
    local closestDist = math.huge
    local character = self.LocalPlayer.Character
    if not character then return end
    
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    for _, player in ipairs(self.Players:GetPlayers()) do
        if player ~= self.LocalPlayer then
            local char = player.Character
            if char then
                local root = char:FindFirstChild("HumanoidRootPart")
                if root then
                    local dist = (root.Position - hrp.Position).Magnitude
                    if dist < closestDist then
                        closestDist = dist
                        closestEnemy = root
                    end
                end
            end
        end
    end
    
    if closestEnemy then
        -- Hedefe doƒüru bak
        hrp.CFrame = CFrame.lookAt(hrp.Position, closestEnemy.Position)
    end
end

-- ===== G√ñRSEL ESP (Geli≈ümi≈ü) [citation:3][citation:8] =====
function BladeBallV5:updateVisuals()
    if not self.Settings.Visuals.Enabled then return end
    
    local character = self.LocalPlayer.Character
    if not character then return end
    
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    -- Top rotasƒ±
    if self.Settings.Visuals.BallTrajectory then
        for _, ball in ipairs(self.BallsFolder:GetChildren()) do
            if ball:IsA("BasePart") then
                local data = self:getBallData(ball)
                if data and data.speed > 5 then
                    -- Rota √ßizgisi
                    local futurePos = ball.Position + data.velocity * 2
                    ESP.drawLine(ball.Position, futurePos, self.Settings.Visuals.Colors.Trajectory)
                    
                    -- Tahmin noktalarƒ±
                    for i = 1, 5 do
                        local time = i * 0.2
                        local pos = ball.Position + data.velocity * time
                        ESP.drawDot(pos, self.Settings.Visuals.Colors.Trajectory, 0.5)
                    end
                end
            end
        end
    end
    
    -- Oyuncu ESP
    if self.Settings.Visuals.PlayerESP then
        for _, player in ipairs(self.Players:GetPlayers()) do
            if player ~= self.LocalPlayer then
                local char = player.Character
                if char then
                    local root = char:FindFirstChild("HumanoidRootPart")
                    if root then
                        local color = self.Settings.Visuals.Colors.Enemy
                        local dist = (root.Position - hrp.Position).Magnitude
                        
                        -- ƒ∞sim
                        ESP.drawText(player.Name, root.Position + Vector3.new(0, 3, 0), color)
                        
                        -- Mesafe
                        if self.Settings.Visuals.DistanceIndicator then
                            ESP.drawText(string.format("%.1fm", dist), root.Position + Vector3.new(0, 2, 0), Color3.fromRGB(255, 255, 255))
                        end
                        
                        -- Health bar
                        if self.Settings.Visuals.HealthBars and char:FindFirstChild("Humanoid") then
                            local hum = char.Humanoid
                            ESP.drawBar(root.Position + Vector3.new(0, 4, 0), hum.Health / hum.MaxHealth, color)
                        end
                    end
                end
            end
        end
    end
end

-- ===== AI √ñƒûRENME MOD√úL√ú [citation:1][citation:8] =====
function BladeBallV5:learn()
    if not self.Settings.AI.Enabled then return end
    
    -- Oyun verilerini topla
    local threats = self:findThreats()
    if #threats == 0 then return end
    
    -- Ba≈üarƒ±lƒ±/ba≈üarƒ±sƒ±z vuru≈ülarƒ± analiz et
    for _, threat in ipairs(threats) do
        local success = (threat.data.timeToImpact > 0.1)  -- Basit ba≈üarƒ± kriteri
        
        table.insert(self.LearningData, {
            time = threat.data.timeToImpact,
            distance = threat.data.distance,
            speed = threat.data.speed,
            success = success
        })
        
        -- Eski verileri temizle
        if #self.LearningData > 100 then
            table.remove(self.LearningData, 1)
        end
    end
    
    -- √ñƒürenme (basit ortalama hesabƒ±)
    if #self.LearningData > 10 and self.Settings.AI.AdaptToPlayer then
        local avgTime = 0
        local successCount = 0
        
        for _, data in ipairs(self.LearningData) do
            avgTime = avgTime + data.time
            if data.success then successCount = successCount + 1 end
        end
        
        avgTime = avgTime / #self.LearningData
        local successRate = successCount / #self.LearningData
        
        -- Prediction time'ƒ± optimize et
        if successRate < 0.8 then
            self.Settings.PredictionTime = math.min(0.6, self.Settings.PredictionTime + 0.01)
        elseif successRate > 0.95 then
            self.Settings.PredictionTime = math.max(0.3, self.Settings.PredictionTime - 0.01)
        end
    end
end

-- ===== GUI OLU≈ûTUR (Nicuse Hub'dan esinlenilmi≈ütir) [citation:4] =====
function BladeBallV5:createGUI()
    local gui = Instance.new("ScreenGui")
    gui.Name = "BladeBallV5"
    gui.Parent = self.CoreGui
    gui.ResetOnSpawn = false
    
    local main = Instance.new("Frame")
    main.Size = UDim2.new(0, 300, 0, 400)
    main.Position = UDim2.new(0.02, 0, 0.25, 0)
    main.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
    main.BackgroundTransparency = 0.1
    main.BorderSizePixel = 0
    main.Parent = gui
    main.Active = true
    main.Draggable = true
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 15)
    corner.Parent = main
    
    -- Ba≈ülƒ±k (RGB efektli)
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 50)
    title.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    title.Text = "‚ö° BLADE BALL V5"
    title.TextColor3 = Color3.new(1, 1, 1)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 22
    title.Parent = main
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 15)
    titleCorner.Parent = title
    
    -- A√ß/Kapa butonu
    local toggleBtn = Instance.new("TextButton")
    toggleBtn.Size = UDim2.new(0.8, 0, 0, 45)
    toggleBtn.Position = UDim2.new(0.1, 0, 0.15, 0)
    toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    toggleBtn.Text = "‚úÖ AKTƒ∞F"
    toggleBtn.TextColor3 = Color3.new(1, 1, 1)
    toggleBtn.Font = Enum.Font.GothamBold
    toggleBtn.TextSize = 18
    toggleBtn.Parent = main
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 10)
    btnCorner.Parent = toggleBtn
    
    -- Mod se√ßici
    local modeBtn = Instance.new("TextButton")
    modeBtn.Size = UDim2.new(0.8, 0, 0, 40)
    modeBtn.Position = UDim2.new(0.1, 0, 0.3, 0)
    modeBtn.BackgroundColor3 = Color3.fromRGB(255, 150, 0)
    modeBtn.Text = "MOD: " .. self.Settings.ParryMode
    modeBtn.TextColor3 = Color3.new(1, 1, 1)
    modeBtn.Font = Enum.Font.Gotham
    modeBtn.TextSize = 16
    modeBtn.Parent = main
    
    local modeCorner = Instance.new("UICorner")
    modeCorner.CornerRadius = UDim.new(0, 10)
    modeCorner.Parent = modeBtn
    
    -- √ñzellik butonlarƒ± (bladesball/blade-ball'dan) [citation:3]
    local features = {"Auto Farm", "Auto Target", "Win Optimizer", "ESP", "AI Learn"}
    local yPos = 0.4
    
    for i, feature in ipairs(features) do
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(0.35, 0, 0, 35)
        btn.Position = UDim2.new(i % 2 == 1 and 0.1 or 0.55, 0, yPos, 0)
        btn.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
        btn.Text = feature
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Font = Enum.Font.Gotham
        btn.TextSize = 14
        btn.Parent = main
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        if i % 2 == 0 then yPos = yPos + 0.08 end
    end
    
    -- ƒ∞statistikler
    local stats = Instance.new("TextLabel")
    stats.Size = UDim2.new(0.9, 0, 0, 60)
    stats.Position = UDim2.new(0.05, 0, 0.75, 0)
    stats.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
    stats.BackgroundTransparency = 0.3
    stats.Text = "‚ö° 31 AKTƒ∞F ‚ö°\nüî• Parry: 0 | Kazanma: %0"
    stats.TextColor3 = Color3.new(1, 1, 1)
    stats.Font = Enum.Font.Gotham
    stats.TextSize = 14
    stats.TextWrapped = true
    stats.Parent = main
    
    local statsCorner = Instance.new("UICorner")
    statsCorner.CornerRadius = UDim.new(0, 10)
