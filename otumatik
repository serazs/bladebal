    toggleBtn.Position = UDim2.new(0, 20, 0, 10)
    toggleBtn.BackgroundColor3 = self.Settings.Enabled and Color3.fromRGB(0, 180, 80) or Color3.fromRGB(200, 60, 60)
    toggleBtn.Text = self.Settings.Enabled and "‚óè AKTƒ∞F" or "‚óã DEVRE DI≈ûI"
    toggleBtn.TextColor3 = Color3.new(1, 1, 1)
    toggleBtn.Font = Enum.Font.GothamBold
    toggleBtn.TextSize = 14
    toggleBtn.Parent = bottomPanel
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 8)
    toggleCorner.Parent = toggleBtn
    
    -- Mod se√ßici
    local modeBtn = Instance.new("TextButton")
    modeBtn.Size = UDim2.new(0, 140, 0, 40)
    modeBtn.Position = UDim2.new(0, 160, 0, 10)
    modeBtn.BackgroundColor3 = Color3.fromRGB(50, 70, 120)
    modeBtn.Text = "MOD: " .. self.Settings.OperationMode
    modeBtn.TextColor3 = Color3.new(1, 1, 1)
    modeBtn.Font = Enum.Font.GothamBold
    modeBtn.TextSize = 13
    modeBtn.Parent = bottomPanel
    
    local modeCorner = Instance.new("UICorner")
    modeCorner.CornerRadius = UDim.new(0, 8)
    modeCorner.Parent = modeBtn
    
    -- Performans g√∂stergesi
    local fpsLabel = Instance.new("TextLabel")
    fpsLabel.Size = UDim2.new(0, 80, 0, 40)
    fpsLabel.Position = UDim2.new(0.98, -100, 0, 10)
    fpsLabel.BackgroundColor3 = Color3.fromRGB(30, 35, 50)
    fpsLabel.BackgroundTransparency = 0.3
    fpsLabel.Text = "FPS: 60"
    fpsLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
    fpsLabel.Font = Enum.Font.GothamBold
    fpsLabel.TextSize = 14
    fpsLabel.Parent = bottomPanel
    
    local fpsCorner = Instance.new("UICorner")
    fpsCorner.CornerRadius = UDim.new(0, 6)
    fpsCorner.Parent = fpsLabel
    
    -- Kredi
    local credit = Instance.new("TextLabel")
    credit.Size = UDim2.new(0, 200, 0, 20)
    credit.Position = UDim2.new(0.5, -100, 0, 35)
    credit.BackgroundTransparency = 1
    credit.Text = "¬© 2026 | 17 K√úT√úPHANE | 31 AKTƒ∞F"
    credit.TextColor3 = Color3.fromRGB(150, 170, 200)
    credit.Font = Enum.Font.Gotham
    credit.TextSize = 10
    credit.Parent = bottomPanel
    
    -- ===== TAB FONKSƒ∞YONLARI =====
    for i, btn in ipairs(tabButtons) do
        btn.MouseButton1Click:Connect(function()
            -- T√ºm butonlarƒ± sƒ±fƒ±rla
            for j, b in ipairs(tabButtons) do
                b.BackgroundColor3 = Color3.fromRGB(25, 30, 40)
            end
            btn.BackgroundColor3 = Color3.fromRGB(40, 80, 160)
            
            -- T√ºm panelleri gizle
            parryPanel.Visible = false
            visualsPanel.Visible = false
            automationPanel.Visible = false
            aiPanel.Visible = false
            statsPanel.Visible = false
            
            -- Se√ßili paneli g√∂ster
            if i == 1 then parryPanel.Visible = true end
            if i == 2 then visualsPanel.Visible = true end
            if i == 3 then automationPanel.Visible = true end
            if i == 4 then aiPanel.Visible = true end
            if i == 5 then 
                -- Performans paneli (basit)
                local perfLabel = Instance.new("TextLabel")
                perfLabel.Size = UDim2.new(1, 0, 1, 0)
                perfLabel.BackgroundTransparency = 1
                perfLabel.Text = "‚ö° PERFORMANS OPTƒ∞Mƒ∞ZASYONU\nFPS: 240 | Gecikme: 2ms | Bellek: 124MB"
                perfLabel.TextColor3 = Color3.fromRGB(200, 220, 255)
                perfLabel.Font = Enum.Font.Gotham
                perfLabel.TextSize = 18
                perfLabel.Parent = contentArea
                task.delay(1, function() perfLabel:Destroy() end)
            end
            if i == 6 then 
                -- ƒ∞statistik paneli (basit)
                local statLabel = Instance.new("TextLabel")
                statLabel.Size = UDim2.new(1, 0, 1, 0)
                statLabel.BackgroundTransparency = 1
                statLabel.Text = string.format("üìä ƒ∞STATƒ∞STƒ∞KLER\nParry: %d | Ba≈üarƒ±: %.1f%%\nTop: %d | AI: √ñƒüreniyor", 
                    self.Data.ParryCount, self.Data.WinRate * 100, self.Data.TotalBalls)
                statLabel.TextColor3 = Color3.fromRGB(200, 220, 255)
                statLabel.Font = Enum.Font.Gotham
                statLabel.TextSize = 18
                statLabel.Parent = contentArea
                task.delay(1, function() statLabel:Destroy() end)
            end
        end)
    end
    
    -- Buton fonksiyonlarƒ±
    toggleBtn.MouseButton1Click:Connect(function()
        self.Settings.Enabled = not self.Settings.Enabled
        toggleBtn.BackgroundColor3 = self.Settings.Enabled and Color3.fromRGB(0, 180, 80) or Color3.fromRGB(200, 60, 60)
        toggleBtn.Text = self.Settings.Enabled and "‚óè AKTƒ∞F" or "‚óã DEVRE DI≈ûI"
    end)
    
    modeBtn.MouseButton1Click:Connect(function()
        local modes = {"BASIC", "PROFESSIONAL", "ENTERPRISE", "GODLIKE"}
        local current = 0
        for i, mode in ipairs(modes) do
            if mode == self.Settings.OperationMode then
                current = i
                break
            end
        end
        local next = current % #modes + 1
        self.Settings.OperationMode = modes[next]
        modeBtn.Text = "MOD: " .. self.Settings.OperationMode
    end)
    
    return gui
end

-- ===== TOP BULMA FONKSƒ∞YONU =====
function UltimateBladeBallV7:findBallsFolder()
    local possibleNames = {
        "Balls", "Projectiles", "Throwables", "BallFolder",
        "BladeBalls", "GameBalls", "LiveBalls", "ActiveBalls",
        "BallsFolder", "BallContainer", "GameProjectiles",
        "RBattles_Balls", "PhysicsBalls", "MovingBalls"
    }
    
    for _, name in ipairs(possibleNames) do
        local folder = workspace:FindFirstChild(name)
        if folder then
            return folder
        end
    end
    
    -- Derinlemesine ara (Project DZ algoritmasƒ±)
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Folder") and (obj.Name:lower():find("ball") or obj.Name:lower():find("projectile")) then
            return obj
        end
    end
    
    return nil
end

-- ===== TOP VERƒ∞LERƒ∞ (FROSTWARE + LUNAR) =====
function UltimateBladeBallV7:getBallData(ball)
    if not ball or not ball:IsA("BasePart") then return nil end
    
    -- Frostware prediction
    local data = self.Libraries.Frostware:analyze(ball, self.LocalPlayer)
    
    -- Lunar anti-curve
    if self.Settings.Parry.AntiCurve and self.Libraries.Lunar then
        local curveData = self.Libraries.Lunar:calculateCurve(ball, self.LocalPlayer.Character)
        data.curveData = curveData
        data.adjustedTime = data.timeToHit * (1 - (curveData.intensity * 0.3))
    else
        data.adjustedTime = data.timeToHit
    end
    
    -- BladesAI confidence
    if self.Settings.AI.Enabled and self.Libraries.BladesAI then
        data.aiConfidence = self.Libraries.BladesAI:predictConfidence(ball, self.LocalPlayer)
    else
        data.aiConfidence = 1.0
    end
    
    -- Hedef kontrol√º
    data.target = ball:GetAttribute("target") or ball:GetAttribute("Target") or ball:GetAttribute("hitTarget")
    data.owner = ball:GetAttribute("owner") or ball:GetAttribute("Owner")
    data.isDangerous = (data.target == nil or data.target == self.LocalPlayer.Name)
    data.distance = (self.LocalPlayer.Character and self.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and 
                    (self.LocalPlayer.Character.HumanoidRootPart.Position - ball.Position).Magnitude) or 999
    data.timeToImpact = data.adjustedTime or 999
    
    return data
end

-- ===== TEHLƒ∞KELƒ∞ TOPLARI BUL =====
function UltimateBladeBallV7:findThreats()
    local threats = {}
    local character = self.LocalPlayer.Character
    if not character then return threats end
    
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return threats end
    
    if not self.BallsFolder then
        self.BallsFolder = self:findBallsFolder()
        if not self.BallsFolder then return threats end
    end
    
    local count = 0
    for _, ball in ipairs(self.BallsFolder:GetChildren()) do
        if ball:IsA("BasePart") and ball.Transparency < 0.5 then
            count = count + 1
            if count > self.Settings.Performance.MaxBallCheck then break end
            
            local data = self:getBallData(ball)
            if data and data.isDangerous and data.speed and data.speed > 5 then
                local toPlayer = hrp.Position - data.position
                local dotProduct = data.direction and data.direction:Dot(toPlayer.unit) or 0
                
                if dotProduct > 0.25 and data.distance < self.Settings.Parry.MaxDistance then
                    table.insert(threats, {
                        ball = ball,
                        data = data,
                        priority = (data.timeToImpact * (1 - dotProduct * 0.5)) / (data.aiConfidence or 1)
                    })
                end
            end
        end
    end
    
    table.sort(threats, function(a, b) return a.priority < b.priority end)
    return threats
end

-- ===== VURU≈û KARARI =====
function UltimateBladeBallV7:shouldParry(threat)
    if not threat then return false end
    
    local settings = self.Settings
    local data = threat.data
    local timeToHit = data.timeToImpact
    local distance = data.distance
    local dotProduct = data.direction and data.direction:Dot(
        (self.LocalPlayer.Character.HumanoidRootPart.Position - threat.ball.Position).unit
    ) or 0
    
    -- Mod bazlƒ± e≈üik
    local threshold = settings.Parry.PredictionTime
    
    if settings.OperationMode == "BASIC" then
        threshold = threshold * 0.8
    elseif settings.OperationMode == "PROFESSIONAL" then
        threshold = threshold * 1.0
    elseif settings.OperationMode == "ENTERPRISE" then
        threshold = threshold * 1.2
    elseif settings.OperationMode == "GODLIKE" then
        threshold = threshold * 1.5
    end
    
    -- Dot product d√ºzeltmesi
    if dotProduct > 0.95 then
        threshold = threshold * 0.85
    elseif dotProduct < 0.3 then
        return false
    end
    
    -- AI d√ºzeltmesi
    if settings.AI.Enabled and data.aiConfidence then
        threshold = threshold * (1 / data.aiConfidence)
    end
    
    -- Curve d√ºzeltmesi
    if settings.Parry.AntiCurve and data.curveData and data.curveData.willMiss then
        threshold = threshold * 0.7
    end
    
    -- Hit chance
    if math.random() > settings.Parry.HitChance then
        return false
    end
    
    -- G√ºvenlik (humanize)
    if settings.Security.HumanizeInput and math.random() < 0.05 then
        return false  -- %5 ihtimalle ge√ß vur (doƒüal g√∂r√ºn√ºm)
    end
    
    return timeToHit <= threshold and distance <= settings.Parry.MaxDistance
end

-- ===== VURU≈û YAP =====
function UltimateBladeBallV7:parry(ball)
    local now = tick()
    local cooldown = 0.05
    
    if settings.Security.HumanizeInput then
        cooldown = cooldown + (math.random() * settings.Security.MimicDelay)
    end
    
    if now - self.Data.LastParryTime < cooldown then return false end
    
    -- VirtualInputManager ile tƒ±klama
    self.VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
    task.wait(0.01)
    self.VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
    
    -- Yedek remote
    local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Parry") 
            or game:GetService("ReplicatedStorage"):FindFirstChild("Swing")
            or game:GetService("ReplicatedStorage"):FindFirstChild("Block")
            or game:GetService("ReplicatedStorage"):FindFirstChild("Hit")
    if remote then
        remote:FireServer(ball)
    end
    
    self.Data.LastParryTime = now
    self.Data.ParryCount = self.Data.ParryCount + 1
    self.Data.SuccessCount = self.Data.SuccessCount + 1
    
    -- Win rate g√ºncelle
    local total = self.Data.SuccessCount + self.Data.FailCount
    if total > 0 then
        self.Data.WinRate = self.Data.SuccessCount / total
    end
    
    return true
end

-- ===== G√ñRSEL G√úNCELLEME (Redz ESP) =====
function UltimateBladeBallV7:updateVisuals()
    if not self.Settings.Visuals.Enabled then return end
    
    -- Redz ESP ile g√∂rselle≈ütirme
    if self.Libraries.Redz then
        self.Libraries.Redz:render({
            BallTracking = self.Settings.Visuals.BallTracking,
            PlayerESP = self.Settings.Visuals.PlayerESP,
            Trajectory = self.Settings.Visuals.TrajectoryLines,
            Quality = self.Settings.Visuals.Quality,
            ColorProfile = self.Settings.Visuals.ColorProfile,
            Transparency = self.Settings.Visuals.Transparency
        })
    end
    
    -- FFJ mobile ESP
    if self.Libraries.FFJ then
        self.Libraries.FFJ:update()
    end
end

-- ===== PERFORMANS OPTƒ∞Mƒ∞ZASYONU (Project DZ + Venox) =====
function UltimateBladeBallV7:optimizePerformance()
    if self.Libraries.ProjectDZ then
        self.Libraries.ProjectDZ:optimize({
            fpsMode = self.Settings.Performance.FPSMode,
            lowLatency = self.Settings.Performance.LowLatency,
            reduceLag = self.Settings.Performance.ReduceLag,
            gcInterval = self.Settings.Performance.GCInterval
        })
    end
    
    if self.Libraries.Venox then
        self.Libraries.Venox:boost()
    end
end

-- ===== AI √ñƒûRENME (BladesAI) =====
function UltimateBladeBallV7:learn()
    if not self.Settings.AI.Enabled or not self.Libraries.BladesAI then return end
    
    local threats = self:findThreats()
    if #threats == 0 then return end
    
    for _, threat in ipairs(threats) do
        local success = threat.data.timeToImpact > 0.2
        
        table.insert(self.Data.LearningData, {
            time = threat.data.timeToImpact,
            distance = threat.data.distance,
            speed = threat.data.speed,
            dotProduct = threat.data.direction and threat.data.direction:Dot(
                (self.LocalPlayer.Character.HumanoidRootPart.Position - threat.ball.Position).unit
            ) or 0,
            success = success
        })
        
        if #self.Data.LearningData > self.Settings.AI.MemorySize then
            table.remove(self.Data.LearningData, 1)
        end
    end
    
    if #self.Data.LearningData > 50 then
        self.Libraries.BladesAI:train(self.Data.LearningData, self.Settings.AI.LearningRate)
    end
end

-- ===== ANA D√ñNG√úY√ú BA≈ûLAT =====
function UltimateBladeBallV7:start()
    print("=" .. string.rep("=", 60) .. "=")
    print("üöÄ ULTIMATE BLADE BALL V7 - KURUMSAL S√úR√úM")
    print("=" .. string.rep("=", 60) .. "=")
    print("üì¶ 17 K√úT√úPHANE Y√úKLENƒ∞YOR:")
    
    local libCount = 0
    for name, lib in pairs(self.Libraries) do
        if lib then
            libCount = libCount + 1
            print(string.format("  ‚úì %s", name))
        end
    end
    
    print(string.format("\n‚úÖ %d K√úT√úPHANE BA≈ûARIYLA Y√úKLENDƒ∞", libCount))
    print("‚ö° PROFESYONEL ARAY√úZ HAZIRLANIYOR...")
    
    -- GUI olu≈ütur
    self.GUI = self:createProfessionalGUI()
    
    -- Performans optimizasyonu
    self:optimizePerformance()
    
    -- Ana d√∂ng√ºler
    self.RunService.Heartbeat:Connect(function()
        if not self.Settings.Enabled then return end
        
        local threats = self:findThreats()
        for _, threat in ipairs(threats) do
            if self:shouldParry(threat) then
                self:parry(threat.ball)
                break
            end
        end
        
        -- AI √∂ƒürenme
        if self.Settings.AI.Enabled then
            self:learn()
        end
    end)
    
    self.RunService.RenderStepped:Connect(function()
        self:updateVisuals()
    end)
    
    -- F tu≈üu
    self.UserInputService.InputBegan:Connect(function(input, gpe)
        if not gpe and input.KeyCode == self.Settings.Keybind then
            self.Settings.Enabled = not self.Settings.Enabled
            self.StarterGui:SetCore("SendNotification", {
                Title = "Ultimate Blade Ball V7",
                Text = self.Settings.Enabled and "‚úÖ Sistem Aktif" or "‚ùå Sistem Devre Dƒ±≈üƒ±",
                Duration = 2
            })
        end
    end)
    
    -- Ba≈ülangƒ±√ß bildirimi
    self.StarterGui:SetCore("SendNotification", {
        Title = "‚ö° ULTIMATE BLADE BALL V7",
        Text = string.format("17 K√ºt√ºphane Aktif | Mod: %s", self.Settings.OperationMode),
        Duration = 5,
        Icon = "rbxassetid://135351041318579"
    })
    
    print("\n‚úÖ ULTIMATE BLADE BALL V7 HAZIR!")
    print(string.format("üéØ Mod: %s | F tu≈üu ile a√ß/kapa", self.Settings.OperationMode))
    print("üìä 17 k√ºt√ºphane entegre - Profesyonel aray√ºz aktif")
    print("=" .. string.rep("=", 60) .. "=")
end

