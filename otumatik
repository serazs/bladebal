-- ===== ANA D√ñNG√úY√ú BA≈ûLAT =====
function BladeBallV5:start()
    print("üöÄ ULTIMATE BLADE BALL V5 BA≈ûLATILIYOR...")
    print("üì¶ Kaynak: GitHub bladesball/blade-ball [citation:3]")
    print("‚ö° √ñzellikler: Auto Parry, ESP, Auto Farm, Win Optimizer, AI Learning")
    
    self.GUI = self:createGUI()
    
    -- GUI buton i≈ülevleri
    self.GUI.toggleBtn.MouseButton1Click:Connect(function()
        self.Settings.Enabled = not self.Settings.Enabled
        self.GUI.toggleBtn.BackgroundColor3 = self.Settings.Enabled and Color3.fromRGB(0, 200, 100) or Color3.fromRGB(200, 0, 0)
        self.GUI.toggleBtn.Text = self.Settings.Enabled and "‚úÖ AKTƒ∞F" or "‚ùå DEVRE DI≈ûI"
    end)
    
    self.GUI.modeBtn.MouseButton1Click:Connect(function()
        if self.Settings.ParryMode == "SAFE" then
            self.Settings.ParryMode = "NORMAL"
        elseif self.Settings.ParryMode == "NORMAL" then
            self.Settings.ParryMode = "AGGRESSIVE"
        elseif self.Settings.ParryMode == "AGGRESSIVE" then
            self.Settings.ParryMode = "INSTANT"
        else
            self.Settings.ParryMode = "SAFE"
        end
        self.GUI.modeBtn.Text = "MOD: " .. self.Settings.ParryMode
    end)
    
    -- Ana d√∂ng√ºler
    self.RunService.Heartbeat:Connect(function()
        if not self.Settings.Enabled then return end
        
        local threats = self:findThreats()
        
        for _, threat in ipairs(threats) do
            if self:shouldParry(threat) then
                self:parry(threat.ball)
                break  -- En tehlikeli topa vur
            end
        end
        
        -- Otomatik √∂zellikler
        if self.Settings.Features.AutoFarm then self:autoFarm() end
        if self.Settings.Features.AutoTarget then self:autoTarget() end
        if self.Settings.AI.Enabled then self:learn() end
    end)
    
    self.RunService.RenderStepped:Connect(function()
        self:updateVisuals()
        
        -- ƒ∞statistikleri g√ºncelle
        if self.GUI and self.GUI.stats then
            self.GUI.stats.Text = string.format(
                "‚ö° 31 AKTƒ∞F ‚ö°\nüî• Parry: %d | Kazanma: %%%.0f",
                self.ParryCount,
                self.WinRate * 100
            )
        end
    end)
    
    -- F tu≈üu ile a√ß/kapa
    self.UserInputService.InputBegan:Connect(function(input, gpe)
        if not gpe and input.KeyCode == self.Settings.Keybind then
            self.Settings.Enabled = not self.Settings.Enabled
            print("‚ö° Blade Ball V5:", self.Settings.Enabled and "AKTƒ∞F" or "DEVRE DI≈ûI")
        end
    end)
    
    -- Karakter yeniden doƒüunca
    self.LocalPlayer.CharacterAdded:Connect(function()
        task.wait(0.5)
        print("üîÑ Karakter yenilendi, script hazƒ±r")
    end)
    
    -- Ba≈ülangƒ±√ß bildirimi
    local notification = Instance.new("TextLabel")
    notification.Size = UDim2.new(0, 400, 0, 100)
    notification.Position = UDim2.new(0.5, -200, 0.8, 0)
    notification.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    notification.BackgroundTransparency = 0.2
    notification.Text = "‚ö° 31 AKTƒ∞F ‚ö°\nULTIMATE BLADE BALL V5\nGitHub: bladesball/blade-ball"
    notification.TextColor3 = Color3.new(1, 1, 1)
    notification.Font = Enum.Font.GothamBold
    notification.TextSize = 20
    notification.TextWrapped = true
    notification.Parent = self.CoreGui
    
    local notifCorner = Instance.new("UICorner")
    notifCorner.CornerRadius = UDim.new(0, 20)
    notifCorner.Parent = notification
    
    task.delay(5, function()
        notification:Destroy()
    end)
    
    print("‚úÖ ULTIMATE BLADE BALL V5 HAZIR!")
    print("üéØ F tu≈üu ile a√ß/kapa | Mod:", self.Settings.ParryMode)
end

-- ===== SCRƒ∞PTƒ∞ BA≈ûLAT =====
local bladeBall = BladeBallV5.new()
bladeBall:start()
